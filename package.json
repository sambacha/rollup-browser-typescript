{
  "name": "rollup-umd-pkg",
  "version": "0.0.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "browser": "dist/bundle.js",
  "types": "dist/bundle.d.ts",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs"
    }
  },
  "typings": "./dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "lib",
    "es"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:es": "tsc --outDir es --module es2015",
    "build:umd": "npm run build:es && rollup --config && dts-bundle --name dist/bundle --main es --outputAsModuleFolder",
    "compile": "npx rollup -c rollup.config.js"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@rollup/plugin-typescript": "^8.3.2",
    "@swc-node/core": "^1.9.0",
    "@swc-node/jest": "^1.5.2",
    "@swc-node/register": "^1.5.1",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/jest": "^27",
    "@types/node": "^16",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@typescript/analyze-trace": "^0.9.0",
    "babel-plugin-macros": "^3.1.0",
    "downlevel-dts": "^0.10.0",
    "dts-bundle": "^0.1.1",
    "esbuild": "^0.14.54",
    "esinstall": "^1.1.7",
    "eslint": "^8.17.0",
    "jest": "^27",
    "prettier": "^2.7.1",
    "process-tracing": "^1.0.2",
    "rollup": "^2.77.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27",
    "tslib": "^2.4.0",
    "tsutils": "^3.21.0",
    "typecheck.macro": "^0.7.3",
    "typedoc": "^0.23.10",
    "typescript": "4.7.3",
    "typescript-eslint-language-service": "^5.0.0",
    "vite": "^3.0.8"
  }
}
